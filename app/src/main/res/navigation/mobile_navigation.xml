<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.kadesa.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.kadesa.ui.pemerintah.PemerintahFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_pemerintah" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.kadesa.ui.pelayanan.PelayananFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_pelayanan" />

    <fragment
        android:id="@+id/navigation_profile"
        android:label="@string/title_notifications"
        android:name="com.example.kadesa.ui.profile.ProfileFragment"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_artikel"
        android:label="@string/title_notifications"
        android:name="com.example.kadesa.ui.artikel.ArtikelFragment"
        tools:layout="@layout/fragment_artikel" />

</navigation>